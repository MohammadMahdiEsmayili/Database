<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.shape.*?>

<AnchorPane prefHeight="485.0" prefWidth="760.0" xmlns="http://javafx.com/javafx/11.0.1"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.NurseOpinionController">
    <children>
        <Rectangle arcHeight="5.0" arcWidth="5.0" fill="DODGERBLUE" height="486.0" stroke="#00a6ff" strokeType="INSIDE"
                   width="760.0"/>
        <Label layoutX="44.0" layoutY="25.0" text="Date of medical examination"/>
        <TextField fx:id="date" layoutX="46.0" layoutY="60.0"/>
        <Label layoutX="272.0" layoutY="25.0" text="Time of medical examination"/>
        <TextField fx:id="time" layoutX="274.0" layoutY="60.0"/>
        <Label layoutX="557.0" layoutY="25.0" text="Medications taken"/>
        <TextField fx:id="medications" alignment="TOP_LEFT" layoutX="491.0" layoutY="60.0" prefHeight="86.0"
                   prefWidth="229.0"/>
        <Label layoutX="157.0" layoutY="214.0" text="Patient condition"/>
        <TextField fx:id="condition" alignment="TOP_LEFT" layoutX="87.0" layoutY="242.0" prefHeight="86.0"
                   prefWidth="230.0"/>
        <Label layoutX="560.0" layoutY="214.0" text="Relevant description"/>
        <TextField fx:id="description" alignment="TOP_LEFT" layoutX="490.0" layoutY="242.0" prefHeight="86.0"
                   prefWidth="230.0"/>
        <Button layoutX="365.0" layoutY="419.0" mnemonicParsing="false" onAction="#onClickOkBtn" text="ok"/>
    </children>
</AnchorPane>
